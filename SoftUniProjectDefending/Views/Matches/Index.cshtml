@{
    ViewData["Title"] = "Matches";
}
<body style="background-color:#b0573c;">
    <div class="text-center">
        <h1 class="display-4", style="color: black">Match Information</h1>
        <p>CompetitionName: @ViewData["competitionName"]</p>
        
        <p style ="float:left">Player 1: @ViewData["player1"]
        </p>

        <p style="float:left">Player 2: @ViewData["player2"]
        </p>

        <p style="float:left">Player 3: @ViewData["player3"]
        </p>

        <p style="float:left">Player 4: @ViewData["player4"]
        </p>

        <p style="float:left">Player 5: @ViewData["player5"]

        </p>
       
